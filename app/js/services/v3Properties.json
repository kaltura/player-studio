[
  {
    "label": "Basic Settings",
    "type": "menu",
    "description": "Basic settings let you set player name, version, entry, and more.",
    "model": "basicSettings",
    "icon": "TabBasicSettings",
    "children": [
      {
        "label": "Player ID:",
        "type": "readonly",
        "player-refresh": false,
        "model": "id"
      },
      {
        "label": "Player Name",
        "type": "text",
        "player-refresh": false,
        "require": true,
        "model": "name",
        "default": "New Player",
        "helpnote": "Add a unique name for the new player",
        "endline": "true"
      },
      {
        "label": "Player Version",
        "type": "playerVersion",
        "model": "playerVersion",
        "resetKalturaPlayer": true,
        "helpnote": "Select a TV Platform Player version - latest or beta. Beta allows you to experiment with new functionalities that will be released in the next version. Latest will always update to the latest version unless you select “Keep Player on Current Version“",
        "initvalue": "latest"
      },
      {
        "label": "Keep Player on Current Version",
        "helpnote": "Prevents the player from automatically updating when a new version becomes available",
        "type": "freezeVersion",
        "endline": "true",
        "filter": "not",
        "resetKalturaPlayer": true,
        "model": "autoUpdate",
        "player-refresh": false
      },
      {
        "label": "Select a Preview Video",
        "type": "select2data",
        "allow-custom-values": true,
        "source": "listEntries",
        "query": "queryEntries",
        "helpnote": "Select a video entry to preview with the new player",
        "player-refresh": true,
        "width": "100%",
        "model": "~selectedEntry",
        "data-placeholder": "Pick an entry",
        "endline": "true"
      },
      {
        "label": "Last updated:",
        "type": "readonly",
        "filter": "timeago",
        "model": "updatedAt"
      }
    ]
  },
  {
    "label": "Playback Options",
    "icon": "playback",
    "description": "Define playback settings.",
    "type": "menu",
    "model": "config",
    "children": [
      {
        "label": "Apply user preferences",
        "helpnote": "Apply settings previously set by the user",
        "type": "checkbox",
        "filter": "not",
        "endline": "true",
        "model": "config.disableUserCache"
      },
      {
        "label": "Captions default display",
        "type": "dropdown",
        "options": [
          {
            "label": "Off",
            "value": "off"
          },
          {
            "label": "Auto",
            "value": "auto"
          },
          {
            "label": "Explicit",
            "value": "explicit"
          }
        ],
        "model": "config.playback.textLanguage",
        "filter": "defaultLanguage",
        "helpnote": "This is the default captions language. Off will not show captions at all. Auto will use the user set preferences from local storage, browser language, or the default language. Explicit requires entering a language code"
      },
      {
        "label": "Language key",
        "type": "captions",
        "model": "languageKey",
        "helpnote": "Deafult captions language",
        "endline": "true"
      },
      {
        "label": "Preload",
        "helpnote": "Load the video asset automatically when the page loads",
        "type": "checkbox",
        "endline": "true",
        "filter": "preload",
        "model": "config.playback.preload"
      },
      {
        "label": "Autoplay",
        "helpnote": "If set to 'Always' - The video will play automatically when the page loads, if set to 'In View' the video will play automatically only if the player is in view. If player is not in view, playback will start only when viewer scrolls to it" ,
        "type": "dropdown",
        "options": [
          {
            "label": "Always",
            "value": "true"
          },
          {
            "label": "In View",
            "value": "inview"
          },
          {
            "label": "Off",
            "value": "false"
          }
        ],
        "filter": "containsBooleans",
        "model": "config.playback.autoplay"
      },
      {
        "label": "Autopause",
        "helpnote": "Pause the video automatically when player scrolls out of view. Resume playback when player goes back into view",
        "type": "checkbox",
        "model": "config.playback.autopause"
      },
      {
        "label": "Start Playback Muted",
        "helpnote": "When playback begins, start video in muted mode",
        "type": "checkbox",
        "model": "config.playback.muted"
      },
      {
        "label": "Fallback to Muted Autoplay",
        "helpnote": "Fall back to muted autoplay on browsers that block autoplay with sound",
        "type": "checkbox",
        "model": "config.playback.allowMutedAutoPlay",
        "endline": "true"
      },
      {
        "label": "Screen lock orientation",
        "helpnote": "Select the default screen lock orientation for the player when in fullscreen",
        "type": "dropdown",
        "model": "config.playback.screenLockOrientionMode",
        "options": [
          {
            "label": "None",
            "value": "none"
          },
          {
            "label": "Any",
            "value": "any"
          },
          {
            "label": "Natural",
            "value": "natural"
          },
          {
            "label": "Landscape",
            "value": "landscape"
          },
          {
            "label": "Portrait",
            "value": "portrait"
          },
          {
            "label": "Portrait primary",
            "value": "portrait-primary"
          },
          {
            "label": "Portrait secondary",
            "value": "portrait-secondary"
          },
          {
            "label": "Landscape primary",
            "value": "landscape-primary"
          },
          {
            "label": "Landscape secondary",
            "value": "landscape-secondary"
          }
        ]
      },
      {
        "label": "Support Youtube playback",
        "helpnote": "When using this option, the player will support playing Youtube ingested content from the platform",
        "type": "checkbox",
        "model": "externals.playkit-youtube.active",
        "componentName": "playkit-youtube",
        "resetKalturaPlayer": true
      }
    ]
  },
  {
    "id": "lookandfeel",
    "label": "Look and Feel",
    "icon": "TabLookandFeel",
    "description": "Adjust the visual appearance of the player.",
    "type": "menu",
    "model": "config",
    "children": {
      "logo": {
        "type": "featuremenu",
        "description": "The Kaltura custom logo plugin.",
        "label": "Logo",
        "model": "config.ui.components.logo",
        "children": [
          {
            "validation": "url",
            "type": "text",
            "label": "Logo image URL",
            "model": "config.ui.components.logo.img",
            "helpnote": "URL for custom control bar logo image."
          },
          {
            "validation": "url",
            "type": "text",
            "label": "Logo link",
            "model": "config.ui.components.logo.url",
            "helpnote": "URL for the control bar logo to click through to."
          },
          {
            "type": "text",
            "label": "Title",
            "model": "config.ui.components.logo.text",
            "helpnote": "Title tooltip for the logo"
          }
        ]
      },
      "bumper": {
        "label": "Bumper",
        "type": "featuremenu",
        "description": "Enable to have a bumper displayed before or after (or both) your content. The bumper could either be attached to an entry (in which case - leave the external url below empty) or provided as an external url.",
        "model": "config.plugins.bumper",
        "componentName": "playkit-bumper",
        "children": [
          {
            "label": "External URL",
            "validation": "url",
            "type": "text",
            "model": "config.plugins.bumper.url",
            "helpnote": "A progressive content url to be used as a bumper"
          }, {
            "label": "Click Through URL",
            "validation": "url",
            "type": "text",
            "model": "config.plugins.bumper.clickThroughUrl"
          }, {
            "label": "Position",
            "type": "dropdown",
            "options": [
              {
                "label": "Pre",
                "value": "pre"
              },
              {
                "label": "Post",
                "value": "post"
              },
              {
                "label": "Both",
                "value": "both"
              }
            ],
            "filter": "bumperPosition",
            "initvalue": "pre",
            "model": "config.plugins.bumper.position",
            "helpnote": "Choose when the bumper should be played"
          }
        ]
      },
      "translation": {
        "type": "featuremenu",
        "description": "Player language selection",
        "checkable": false,
        "enabled": true,
        "isOpen": true,
        "label": "Localization",
        "children": [
          {
            "label": "Languages",
            "helpnote": "Select all languages to be bundled with the player",
            "player-refresh": false,
            "type": "dropdown",
            "options": [
              {
                "label": "English",
                "value": "en"
              },
              {
                "label": "German",
                "value": "de"
              },
              {
                "label": "French",
                "value": "fr"
              },
              {
                "label": "Spanish",
                "value": "es"
              },
              {
                "label": "Italian",
                "value": "it"
              },
              {
                "label": "Dutch",
                "value": "nl"
              },
              {
                "label": "Russian",
                "value": "ru"
              },
              {
                "label": "Portuguese (Brazilian)",
                "value": "pt_br"
              },
              {
                "label": "Japanese",
                "value": "ja"
              },
              {
                "label": "Chinese (Simplified)",
                "value": "zh_cn"
              },
              {
                "label": "Chinese (Traditional)",
                "value": "zh_tw"
              },
              {
                "label": "Hindi",
                "value": "hi_in"
              },
              {
                "label": "Arabic",
                "value": "ar"
              },
              {
                "label": "Hebrew",
                "value": "he"
              },
              {
                "label": "Korean",
                "value": "ko"
              },
              {
                "label": "Canadian French",
                "value": "fr_ca"
              },
              {
                "label": "Finnish",
                "value": "fi"
              }
            ],
            "model": "playerLangCodes",
            "filter": "array",
            "multiple": true
          },
          {
            "label": "Default Language",
            "helpnote": "Select the default language for the player",
            "type": "bindDropdown",
            "model": "config.ui.locale",
            "filter": "noEmpty",
            "options": "playerLang"
          }
        ]
      },
      "floating": {
        "type": "featuremenu",
        "description": "When a viewer scrolls the player out of view, a floating player would pop-up and position itself following the configuration. This allows the viewer to engage with both the video content and the article.",
        "model": "config.plugins.floating",
        "componentName": "playkit-visibility",
        "label": "Floating",
        "children": [
          {
            "label": "Position",
            "type": "dropdown",
            "model": "config.plugins.floating.position",
            "options": [
              {
                "label": "Top Right",
                "value": "top-right"
              },              {
                "label": "Bottom Right",
                "value": "bottom-right"
              },
              {
                "label": "Top Left",
                "value": "top-left"
              },              {
                "label": "Bottom Left",
                "value": "bottom-left"
              }
            ],
            "initvalue": "bottom-right",
            "helpnote": "The position where the floating player will be displayed"
          },
          {
            "label": "Width",
            "type": "number",
            "model": "config.plugins.floating.width",
            "helpnote": "The width of the floating player in pixels",
            "min": 0,
            "initvalue": 400,
            "filter": "numberOnly"
          },
          {
            "label": "Height",
            "type": "number",
            "model": "config.plugins.floating.height",
            "helpnote": "The height of the floating player in pixels",
            "min": 0,
            "initvalue": 225,
            "filter": "numberOnly"
          },
          {
            "label": "Margin-X",
            "type": "number",
            "model": "config.plugins.floating.marginX",
            "helpnote": "The margin, in pixels, from the selected edge, on the X-Axis",
            "min": 0,
            "initvalue": 0,
            "filter": "numberOnly"
          },
          {
            "label": "Margin-Y",
            "type": "number",
            "model": "config.plugins.floating.marginY",
            "helpnote": "The margin, in pixels, from the selected edge, on the Y-Axis",
            "min": 0,
            "initvalue": 0,
            "filter": "numberOnly"
          },
          {
            "label": "Dismissible",
            "type": "checkbox",
            "model": "config.plugins.floating.dismissible",
            "helpnote": "When set to true, viewer will be able to dismiss the floating player so that it doesn’t appear anymore while he scrolls the current page",
            "initvalue": true
          },
          {
            "label": "Draggable",
            "type": "checkbox",
            "model": "config.plugins.floating.draggable",
            "helpnote": "When set to true, viewer will be able to drag the floating player. Uncheck if you want to have a fixed location for the floating player",
            "initvalue": true
          }
        ]
      },
      "share": {
