  {
    "label": "Theming",
    "icon": "theming",
    "description": "Adjust the styling of the player.",
    "type": "menu",
    "children": [
          {
            "label": "External CSS",
            "type": "text",
            "model": "config.ui.css",
            "filter": "noEmpty",
            "helpnote": "Player will load the given CSS file using this URL"
        }
    ]
  },
  {
    "label": "Analytics & Monetization",
    "icon": "TabAnalytics",
    "description": "",
    "type": "menu",
    "children": {
      "youbora": {
        "label": "Youbora",
        "type": "featuremenu",
        "description": "A powerful intelligence analytics and optimization platform to deliver online TV-quality video experiences. For full options see <a href='http://developer.nicepeopleatwork.com/apidocs/js/$YB.data.Options.html' target='_blank'>Here</a>.<br>",
        "model": "config.plugins.youbora",
        "endline": "true",
        "componentName": "playkit-youbora",
        "children": [
          {
            "label": "Account code",
            "type": "text",
            "model": "config.plugins.youbora.options",
            "helpnote": "Enter your Youbora account ID",
            "filter": "accountCode"
          }
        ]
      },
      "googleAnalytics": {
        "label": "Google Analytics",
        "type": "featuremenu",
        "description": "Supports sending player analytics events to Google. For full configuration options see <a href='https://github.com/kaltura/playkit-js-google-analytics/blob/master/docs/configuration.md' target='_blank'>Google Analytics config file</a>.<br>",
        "model": "config.plugins.googleAnalytics",
        "endline": "true",
        "componentName": "playkit-google-analytics",
        "children": [
          {
            "label": "Universal Analytics Account ID",
            "type": "text",
            "model": "config.plugins.googleAnalytics.trackingId",
            "helpnote": "The Universal Analytics Account ID i.e. UA-30164991-1"
          },
          {
            "label": "Google Analytics 4 data stream ID",
            "type": "text",
            "model": "config.plugins.googleAnalytics.trackingGA4Id",
            "helpnote": "The Google Analytics 4 data stream ID (aka GA4 Measurement ID) i.e. G-4XZ9KWPHEC"
          }
        ]
      },
      "comscore": {
        "label": "comScore",
        "type": "featuremenu",
        "description": "Integrates comScore Streaming Tag data collection.<br>Please contact your comScore account team for complete instructions for using these parameters, which will be tailored to the needs of the reporting product you aim to collect data for.",
        "model": "config.plugins.comscore",
        "endline": "true",
        "componentName": "playkit-comscore",
        "children": [
          {
            "label": "Publisher ID",
            "type": "text",
            "model": "config.plugins.comscore.publisherId",
            "helpnote": "A comScore-provided number with at least 7 digits that is unique for each Publisher. The Publisher ID is also known as the 'Client ID' or 'c2' parameter value"
          },
          {
            "label": "Label Mapping",
            "type": "text",
            "model": "config.plugins.comscore.labelMapping",
            "helpnote": "A comma-separated list of 'name=source' pairs where the source values will be retrieved each time the player changes media and included in data transmissions from the plugin"
          },
          {
            "label": "Persistent Labels",
            "type": "text",
            "model": "config.plugins.comscore.persistentLabels",
            "helpnote": "A comma-separated list of 'name=value' pairs which will be treated as static values and included in all data transmissions from the plugin"
          }
        ]
      },
      "ima": {
        "label": "IMA",
        "type": "featuremenu",
        "description": "The Google IMA SDK for HTML5 V3 allows developers to request and track VAST ads in a HTML5 video environment. For full APIs see: <br><a href='https://developers.google.com/interactive-media-ads/docs/sdks/html5' target='_blank'>Google IMA HTML5 SDK APIs</a>",
        "model": "config.plugins.ima",
        "componentName": "playkit-ima",
        "children": [
          {
            "label": "Ad tag URL",
            "validation": "url",
            "type": "text",
            "model": "config.plugins.ima.adTagUrl",
            "helpnote": "Enter a URL for the advertising tag"
          },
          {
            "label": "Loading Timeout",
            "type": "number",
            "model": "config.plugins.ima.adsRenderingSettings",
            "helpnote": "Set the timeout for loading ads (in milliseconds). If an ad can’t be loaded within this timeframe, load the video automatically",
