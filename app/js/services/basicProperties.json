                "model": "name",
                "default": "New Player",
                "helpnote": "This can't be numbers",
                "endline": "true"
            },
            {
                "label": "Some Custom URL",
                "type": "text",
                "require": true,
                "validation": "url",
                "model": "url",
                "default": "New Player",
                "helpnote": "Please enter a URL",
                "endline": "true"
            },
            {
                "label": "email",
                "type": "text",
                "validation": "email",
                "model": "email",
                "default": "New Player",
                "helpnote": "Please enter email (not required)",
                "endline": "true"
            }
        ]
    }
